; -*- mode: lisp; -*-
;
; create a corner at ($PosX $PosY $PosZ $Orientation) named $Name with the
; given $Material
;

(RSG 0 1)
(
    (templ $PosX $PosY $Orientation)

    (nd Transform
        (setLocalPos $PosX $PosY 0)
        (nd FieldFeature
           (setPosition $PosX $PosY 0)
           (setOrientation $Orientation)
           (setType CORNER)
        )
    )
)